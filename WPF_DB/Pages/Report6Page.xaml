<fuic:Page x:Class="WPF_DB.Pages.Report6Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:fuic="clr-namespace:FloxelLib.Controls;assembly=Floxel"
      xmlns:mvvm="clr-namespace:WPF_DB.MVVM"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Report6Page">

    <Page.DataContext>
        <mvvm:ReportViewModel/>
    </Page.DataContext>

    <Grid>
        <WindowsFormsHost Width="700" HorizontalAlignment="Left" x:Name="winformsHost" Margin="2"/>
        <Button Width="80" Height="40" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Margin="0,0,10,48" Content="назад" Command="{Binding MenuCommand}"/>
    </Grid>
</fuic:Page>
